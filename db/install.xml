<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/jsinjection/db" VERSION="2019052800" COMMENT="XMLDB file for Moodle blocks/jsinjection"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_jsinjection_conf" COMMENT="jsinjection block visibility settings ui">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="categoryid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Moodle course category id"/>
        <FIELD NAME="datatype" TYPE="char" NOTNULL="true" LENGTH="20" SEQUENCE="false" COMMENT="What identifier from the user data table is to be displayed."/>
        <FIELD NAME="userid"  TYPE="int" NOTNULL="true" LENGTH="10" SEQUENCE="false" COMMENT="User that added the setting"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
        <KEY NAME="categoryid" TYPE="foreign" FIELDS="categoryid" REFTABLE="course_categories" REFFIELDS="id"/>
        <KEY NAME="userid" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
